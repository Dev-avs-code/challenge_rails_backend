{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Maintenance services Index Response",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "attributes": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "description": {
                "type": "string"
              },
              "status": {
                "enum": [
                  "pending",
                  "in_progress",
                  "completed"
                ]
              },
              "cost_cents": {
                "type": "string"
              },
              "priority": {
                "enum": [
                  "low",
                  "medium",
                  "high"
                ]
              },
              "completed_at": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "created_at": {
                "type": "string"
              },
              "updated_at": {
                "type": "string"
              }
            },
            "required": [
              "description",
              "status",
              "cost_cents",
              "priority",
              "completed_at",
              "created_at",
              "updated_at"
            ]
          }
        }
      }
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        },
        "first": {
          "type": "string"
        },
        "prev": {
          "type": [
            "string",
            "null"
          ]
        },
        "next": {
          "type": [
            "string",
            "null"
          ]
        },
        "last": {
          "type": "string"
        }
      },
      "required": [
        "self",
        "first",
        "prev",
        "next",
        "last"
      ]
    }
  },
  "required": [
    "data",
    "links"
  ]
}