{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Maintenance service Show Response",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "description": {
              "type": "string"
            },
            "status": {
              "enum": [
                "pending",
                "in_progress",
                "completed"
              ]
            },
            "cost-cents": {
              "type": "string"
            },
            "priority": {
              "enum": [
                "low",
                "medium",
                "high"
              ]
            },
            "completed-at": {
              "type": [
                "string",
                "null"
              ],
              "format": "date-time"
            },
            "created-at": {
              "type": "string",
              "format": "date-time"
            },
            "updated-at": {
              "type": "string",
              "format": "date-time"
            }
          },
          "required": [
            "description",
            "status",
            "cost-cents",
            "priority",
            "completed-at",
            "created-at",
            "updated-at"
          ]
        }
      },
      "required": [
        "id",
        "type",
        "attributes"
      ]
    }
  },
  "required": [
    "data"
  ]
}