{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Login or create account Response",
  "type": "object",
  "properties": {
    "data": {
      "properties": {
        "message": {
          "type": "string"
        },
        "token": {
          "type": "string",
          "minLength": 20
        },
        "token_type": {
          "type": "string"
        },
        "expired_at": {
          "type": "number"
        },
        "user": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "email": {
              "type": "string",
              "format": "email"
            }
          },
          "required": [
            "id",
            "email"
          ]
        }
      },
      "required": [
        "message",
        "token",
        "token_type",
        "expired_at"
      ]
    }
  },
  "required": [
    "data"
  ]
}