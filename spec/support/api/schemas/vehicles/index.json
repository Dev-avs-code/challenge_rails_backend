{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Vehicles Index Response",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "attributes": {
            "type": "object",
            "properties": {
              "vin": {
                "type": "string"
              },
              "plate": {
                "type": "string"
              },
              "brand": {
                "type": "string"
              },
              "model": {
                "type": "string"
              },
              "year": {
                "type": "string"
              },
              "status": {
                "enum": [
                  "active",
                  "in_maintenance",
                  "inactive"
                ]
              },
              "created-at": {
                "type": "string",
                "format": "date-time"
              },
              "updated-at": {
                "type": "string",
                "format": "date-time"
              }
            },
            "required": [
              "vin",
              "plate",
              "brand",
              "model",
              "year",
              "status",
              "created-at",
              "updated-at"
            ]
          }
        },
        "required": [
          "id",
          "type",
          "attributes"
        ]
      }
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        },
        "first": {
          "type": "string"
        },
        "prev": {
          "type": [
            "string",
            "null"
          ]
        },
        "next": {
          "type": [
            "string",
            "null"
          ]
        },
        "last": {
          "type": "string"
        }
      },
      "required": [
        "self",
        "first",
        "prev",
        "next",
        "last"
      ]
    }
  },
  "required": [
    "data",
    "links"
  ]
}